    "openai": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "stmux": "^1.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "wait-port": "^1.0.1"
  },
  "scripts": {
    "dev:functions": "nodemon -r dotenv/config ./index.ts",
