{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "nitric typescript starter template",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "@nitric/sdk": "0.10.0",
    "@supabase/supabase-js": "^2.20.0",
    "common-tags": "^1.8.2",
    "github-slugger": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
