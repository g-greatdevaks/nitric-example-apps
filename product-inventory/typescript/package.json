{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "nitric typescript starter template",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-ses": "^3.121.0",
    "@nitric/sdk": "latest",
    "aws-sdk": "^2.1363.0",
    "mustache": "^4.2.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "dotenv": "^16.0.2",
    "nodemon": "^2.0.20",
    "stmux": "^1.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "wait-port": "^1.0.1"
  },
  "scripts": {
    "dev:functions": "nodemon -r dotenv/config ./index.ts",
    "dev": "stmux -w always -e ERROR -M -m beep,system -- [ -t 'Nitric: Exit using ctrl^a+k' \"nitric start\" .. -t 'Functions' \"wait-port localhost:50051 && npm run dev:functions\" ]"
  }
}
